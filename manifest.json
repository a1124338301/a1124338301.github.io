{
	"name": "Pokemon Alpha",
	"description": "A simple pokemon game",
	"version": "1.0.0.0",
	"manifest_version": 2,
	"app": {
		"background": {
			"scripts": ["background.js"],
			"container": "panel",
			"width": 780,
			"height": 520
		}
	},
	"icons": {
		"16": "icon-16.png",
		"128": "icon-128.png",
		"256": "icon-256.png"
	},
	"permissions": [
		"storage",
		"unlimitedStorage"
	],
	"offline_enabled": true
}